{
  "connector": "local",
  "extends": ["accessibility", "progressive-web-apps"],
  "formatters": ["html", "summary"],
  "hints": {
    "axe": "error",
    "babel-config/is-valid": "error",
    "disown-opener": "error",
    "highest-available-document-mode": "error",
    "manifest-exists": "off",
    "meta-charset-utf-8": "error",
    "meta-viewport": "error"
  },
  "hintsTimeout": 10000
}
